// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Files {
  fileID String @id @default(uuid())
  userID Int 
  filename String
  url String
}

model Warns {
  warnID String @id @default(uuid())
  userID Int
  modID Int
  reason String
}

model Github {
  userID Int @id
  token Int 
}
